{
  "name": "sassy-authy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate:prisma-authenticate": "npx prisma generate --schema=./prisma/authenticate/schema.prisma",
    "reset:prisma-authenticate": "npx prisma migrate reset --schema=./prisma/authenticate/schema.prisma",
    "push:prisma-authenticate": "npx prisma db push --schema=./prisma/authenticate/schema.prisma",
    "studio:prisma-authenticate": "npx prisma studio --schema=./prisma/authenticate/schema.prisma",
    "prisma-all": "npm run generate:prisma-authenticate && npm run push:prisma-authenticate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@hookform/resolvers": "^3.3.3",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.10.6",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.303.0",
    "next": "14.0.4",
    "next-auth": "^5.0.0-beta.4",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.12.0",
    "react-spinners": "^0.13.8",
    "resend": "^2.1.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.3.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "prisma": "^5.7.1"
  }
}
